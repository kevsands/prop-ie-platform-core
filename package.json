{
  "name": "prop-ie-aws-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema-unified.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema-unified.prisma",
    "db:seed": "node scripts/seed-enterprise-database.js",
    "db:seed-basic": "node scripts/seed-database.js",
    "db:studio": "prisma studio --schema=./prisma/schema-unified.prisma",
    "build:staging": "cross-env NODE_ENV=staging next build --no-lint",
    "build:prod": "cross-env NODE_ENV=production next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "test": "npx jest --no-cache",
    "test:ci": "npx jest --ci --coverage",
    "test:integration": "npx jest --config jest.integration.config.js",
    "test:integration:ci": "npx jest --config jest.integration.config.js --ci --coverage",
    "test:coverage": "npx jest --coverage",
    "test:coverage-report": "node scripts/generate-coverage-report.js",
    "test:watch": "npx jest --watch",
    "test:amplify": "npx jest --testPathPattern=amplify",
    "test:performance": "npx jest --testPathPattern=performance",
    "test:regression": "npx jest --testPathPattern=performance/regressionTests",
    "test:health": "node scripts/test-health-dashboard.js",
    "test:health:ci": "node scripts/test-health-dashboard.js --ci",
    "test:example": "npx jest --no-coverage __tests__/examples/standardized-test-example.tsx",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cypress:run",
    "build:worker": "echo 'Worker build has been disabled'",
    "preview": "next dev",
    "export": "next build && next export -o out",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "security-check": "npm audit && npx eslint . --ext .js,.jsx,.ts,.tsx --config .eslintrc.security.js && ([ -f ./njsscan-wrapper.sh ] && ./njsscan-wrapper.sh . || echo 'njsscan not installed, skipping')",
    "verify-lockfile": "node scripts/verify-package-lock.js",
    "preinstall": "node scripts/verify-package-lock.js",
    "postinstall": "node scripts/suspicious-package-check.js || true",
    "generate-sbom": "npx @cyclonedx/cyclonedx-npm --output sbom.json",
    "validate-sbom": "node scripts/validate-sbom.js",
    "dependency-scan": "npm audit && npx is-website-vulnerable && npx snyk test || true",
    "analyze-package": "node scripts/analyze-package.js",
    "analyze-bundle": "bash scripts/analyze-bundle.sh",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "cross-env ANALYZE=true next build",
    "migrate:postgres": "bash scripts/migrate-to-postgresql.sh",
    "load:test": "node scripts/load-testing.js",
    "load:test:production": "TEST_URL=https://prop.ie TOTAL_REQUESTS=100000 CONCURRENT_USERS=500 node scripts/load-testing.js",
    "db:seed:production": "NODE_ENV=production node scripts/seed-production-data.js",
    "deploy:staging": "npm run build:staging && aws amplify start-deployment --app-id $AMPLIFY_APP_ID --branch staging",
    "deploy:production": "npm run build:prod && aws amplify start-deployment --app-id $AMPLIFY_APP_ID --branch production",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "security:audit": "npm audit --audit-level moderate && npx snyk test",
    "performance:audit": "npx lighthouse-ci autorun --config=.lighthouserc.js"
    "test:platform": "node scripts/run-all-tests.js",
    "test:db": "node scripts/test-database-connectivity.js",
    "test:jwt": "node scripts/test-jwt-tokens.js",
    "test:core": "node scripts/test-core-functionality.js",
    "db:backup": "node database/scripts/01_sqlite_backup.js",
    "db:test-postgresql": "node database/scripts/02_postgresql_test.js",
    "db:migration-prepare": "node database/scripts/enterprise_migration.js",
    "db:migrate-data": "node database/scripts/03_data_migration.js",
    "db:config-update": "node database/scripts/04_application_configuration.js",
    "test:htb-postgresql": "node database/scripts/test_htb_postgresql.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "a11y-audit": "node scripts/accessibility-audit.js",
    "a11y-audit:ci": "node scripts/accessibility-audit.js --ci",
    "a11y-audit:fix": "node scripts/accessibility-audit.js --fix",
    "performance:analyze": "cross-env ANALYZE=true next build",
    "performance:test": "npx lighthouse https://localhost:3000 --view",
    "performance:ci": "npx lighthouse-ci https://preview-url.amplifyapp.com --output=json",
    "coverage:badge": "node scripts/generate-coverage-report.js --badges-only",
    "coverage:trend": "node scripts/generate-coverage-report.js --update-history",
    "analyze-performance": "node scripts/analyze-performance.js",
    "docker:dev": "docker-compose up -d",
    "docker:dev:build": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:analyze": "docker-compose --profile analyze up -d",
    "prepare:aws": "node scripts/verify-aws-config.js",
    "setup:amplify": "node scripts/setup-amplify-environments.js",
    "typescript:check": "node scripts/typescript-check.js",
    "lint:fix": "next lint --fix",
    "dev:secure": "cross-env SECURITY_ENHANCED=true next dev",
    "build:secure": "cross-env SECURITY_ENHANCED=true next build",
    "clean": "rm -rf .next node_modules/.cache",
    "deploy:staging": "cross-env NODE_ENV=staging npx amplify publish -y",
    "coverage:dashboard": "node scripts/coverage-dashboard.js",
    "coverage:dashboard:save": "node scripts/coverage-dashboard.js --save",
    "coverage:demo": "node scripts/demo-coverage-dashboard.js",
    "coverage:simple": "node scripts/coverage-dashboard-simple.js",
    "test:with-dashboard": "npm test -- --coverage && npm run coverage:dashboard:save",
    "ci:test": "npm test -- --coverage && npm run coverage:dashboard:save",
    "db:connect-postgresql": "node database/scripts/connect_postgresql.js",
    "db:status": "node database/scripts/02_postgresql_test.js",
    "db:backup-postgresql": "pg_dump $PG_DATABASE > database/backups/postgresql_backup_$(date +%Y-%m-%d_%H-%M-%S).sql"
  },
