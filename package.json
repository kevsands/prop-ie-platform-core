    "preinstall": "node scripts/verify-package-lock.js",
    "generate-sbom": "npx @cyclonedx/cyclonedx-npm --output sbom.json",
    "validate-sbom": "node scripts/validate-sbom.js",
    "dependency-scan": "npm audit && npx is-website-vulnerable && npx snyk test || true",
    "analyze-package": "node scripts/analyze-package.js",
    "analyze-bundle": "bash scripts/analyze-bundle.sh",
    "analyze": "ANALYZE=true npm run build",
    "test:platform": "node scripts/run-all-tests.js",
    "test:db": "node scripts/test-database-connectivity.js",
    "test:jwt": "node scripts/test-jwt-tokens.js",
    "test:core": "node scripts/test-core-functionality.js",
