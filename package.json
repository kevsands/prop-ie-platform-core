    "test:with-dashboard": "npm test -- --coverage && npm run coverage:dashboard:save",
    "ci:test": "npm test -- --coverage && npm run coverage:dashboard:save",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "remove-console": "node scripts/remove-console-statements.js",
    "remove-console:dry-run": "node scripts/remove-console-statements.js --dry-run"
  },
