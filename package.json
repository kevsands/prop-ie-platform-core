  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "backup-files": "node scripts/backup-files-manager.js",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:prod": "node scripts/setup-production-database.js",
    "db:health": "node scripts/db-health-check.js",
