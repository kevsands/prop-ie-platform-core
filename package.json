    "db:seed": "node scripts/seed-database.js",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "build:staging": "cross-env NODE_ENV=staging next build --no-lint",
    "build:prod": "cross-env NODE_ENV=production next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "test": "npx jest --no-cache",
    "test:ci": "npx jest --ci --coverage",
    "test:integration": "npx jest --config jest.integration.config.js",
    "test:integration:ci": "npx jest --config jest.integration.config.js --ci --coverage",
    "test:coverage": "npx jest --coverage",
    "test:coverage-report": "node scripts/generate-coverage-report.js",
    "test:coverage-dashboard": "node scripts/generate-test-coverage-dashboard.js",
