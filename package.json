    "claude:fix": "./scripts/claude-code-review.sh fix",
    "claude:perf": "./scripts/claude-code-review.sh perf",
    "claude:types": "./scripts/claude-code-review.sh types",
    "claude:docs": "./scripts/claude-code-review.sh docs",
